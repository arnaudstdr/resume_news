{
  "name": "resume_ai",
  "image": "mcr.microsoft.com/devcontainers/python:3.10-bullseye",
  "runArgs": ["--name", "resume_ai"],
  "postCreateCommand": "pip install -r requirements.txt",
  "customizations": {
    "vscode": {
      "extensions": [
        "ms-python.python",
        "ms-toolsai.jupyter",
        "ms-azuretools.vscode-docker"
      ]
    }
  },
  "features": {},
  "forwardPorts": [8888],

  // Container user
  "remoteUser": "vscode"
}